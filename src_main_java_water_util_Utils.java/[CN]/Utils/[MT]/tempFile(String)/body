{
  File file;
  FileWriter w=null;
  try {
    file=File.createTempFile("h2o",null);
    w=new FileWriter(file);
    w.write(content);
  }
 catch (  IOException ex) {
    throw new RuntimeException(ex);
  }
 finally {
    if (w != null) {
      try {
        w.close();
      }
 catch (      IOException _) {
      }
    }
  }
  return file;
}
