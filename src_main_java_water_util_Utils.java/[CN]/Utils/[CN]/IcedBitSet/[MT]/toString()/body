{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  boolean first=true;
  for (int i=0; i < _val.length; i++) {
    if (_val[i] == 0)     continue;
    for (int j=0; j < 8; j++) {
      if ((_val[i] & ((byte)1 << j)) == 1) {
        if (!first)         sb.append(", ");
        sb.append(8 * i + j);
        first=false;
      }
    }
  }
  sb.append("}");
  return sb.toString();
}
