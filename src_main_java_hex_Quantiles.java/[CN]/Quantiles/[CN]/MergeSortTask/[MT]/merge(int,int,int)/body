{
  for (int i=low; i <= high; i++) {
    _tmp2[i]=_tmp[i];
  }
  int i=low;
  int j=middle + 1;
  int k=low;
  for (; i <= middle && j <= high; k++) {
    if (_tmp2[i] <= _tmp2[j]) {
      _tmp2[k]=_tmp[i++];
    }
 else {
      _tmp2[k]=_tmp[j++];
    }
  }
  for (; i <= middle; k++, i++) {
    _tmp[k]=_tmp2[i];
  }
}
