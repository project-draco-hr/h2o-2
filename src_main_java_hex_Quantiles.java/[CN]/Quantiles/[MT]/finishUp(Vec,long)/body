{
  _pctile=new double[QUANTILES_TO_DO.length];
  if (_isEnum) {
    ;
  }
 else {
    if (!_multiPass) {
      _done=approxQuantilesOnePass(_pctile,QUANTILES_TO_DO);
    }
 else {
      long desiredBinCnt=max_qbins + 1;
      _done=exactQuantilesMultiPass(_pctile,QUANTILES_TO_DO,desiredBinCnt);
    }
  }
}
