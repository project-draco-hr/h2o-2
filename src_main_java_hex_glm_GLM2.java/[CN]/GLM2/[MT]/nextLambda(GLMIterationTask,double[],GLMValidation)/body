{
  currentLambdaIter=0;
  boolean improved=_model.setAndTestValidation(_lambdaIdx,val);
  _model.clone().update(self());
  if ((improved || _runAllLambdas) && _lambdaIdx < (lambda.length - 1)) {
    ++_lambdaIdx;
    glmt._val=null;
    new Iteration().callback(glmt);
  }
 else   GLM2.this.complete();
}
