{
  System.err.println(getPendingCount());
  dataset.unlock(jobKey);
  try {
    for (    Frame s : splits) {
      if (!exceptional) {
        s.update(jobKey);
        s.unlock(jobKey);
      }
 else {
        if (s != null)         s.delete(jobKey,0f);
      }
    }
  }
 catch (  Throwable e) {
    e.printStackTrace();
  }
}
