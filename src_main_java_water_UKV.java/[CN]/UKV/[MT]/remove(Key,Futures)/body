{
  Value val=DKV.get(key,32,H2O.GET_KEY_PRIORITY);
  if (val == null)   return;
  if (val.isArray()) {
    ValueArray ary=val.get();
    for (long i=0; i < ary.chunks(); i++)     DKV.remove(ary.getChunkKey(i),fs);
  }
  if (key._kb[0] == Key.VEC) {
    water.fvec.Vec vec=val.get();
    for (int i=0; i < vec.nChunks(); i++)     remove(vec.chunkKey(i),fs);
  }
  if (val.isFrame())   ((Frame)val.get()).remove(fs);
  DKV.remove(key,fs);
}
