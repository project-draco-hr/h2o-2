{
  String s="{" + _names[0];
  long bs=_vecs[0].byteSize();
  for (int i=1; i < _names.length; i++) {
    s+="," + _names[i];
    bs+=_vecs[i].byteSize();
  }
  s+="}, " + PrettyPrint.bytes(bs) + "\n";
  Vec v0=anyVec();
  if (v0 == null)   return s;
  int nc=v0.nChunks();
  s+="Chunk starts: {";
  for (int i=0; i < nc; i++)   s+=v0.elem2BV(i)._start + ",";
  s+="}";
  return s;
}
