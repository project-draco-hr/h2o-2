{
  Vec vecs[]=vecs();
  if (vecs.length == 0)   return "{}";
  String s="{" + _names[0];
  long bs=vecs[0].byteSize();
  for (int i=1; i < vecs.length; i++) {
    s+="," + _names[i];
    bs+=vecs[i].byteSize();
  }
  s+="}, " + PrettyPrint.bytes(bs) + "\n";
  Vec v0=anyVec();
  if (v0 == null)   return s;
  int nc=v0.nChunks();
  s+="Chunk starts: {";
  for (int i=0; i < nc; i++)   s+=v0.chunkForChunkIdx(i)._start + ",";
  s+="}";
  return s;
}
