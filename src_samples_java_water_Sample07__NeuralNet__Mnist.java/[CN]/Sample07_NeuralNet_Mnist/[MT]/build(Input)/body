{
  Layer[] ls=new Layer[3];
  ls[0]=input;
  ls[1]=new Layer.Tanh();
  ls[2]=new Layer.Softmax();
  ls[1]._rate=.05f;
  ls[2]._rate=.02f;
  ls[1]._l2=.0001f;
  ls[2]._l2=.0001f;
  ls[1]._rateAnnealing=1 / 2e6f;
  ls[2]._rateAnnealing=1 / 2e6f;
  ls[1].init(ls[0],500);
  ls[2].init(ls[1],10);
  for (int i=1; i < ls.length; i++)   ls[i].randomize();
  return ls;
}
