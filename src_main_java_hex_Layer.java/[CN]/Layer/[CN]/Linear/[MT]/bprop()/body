{
  long processed=_training.processed();
  float m=momentum(processed);
  float r=rate(processed) * (1 - m);
  float[] v=target();
  for (int u=0; u < _a.length; u++) {
    float g=v[u] - _a[u];
    if (loss == NeuralNet.NeuralNetParams.Loss.CrossEntropy) {
    }
 else     if (loss == NeuralNet.NeuralNetParams.Loss.MeanSquare) {
      g*=(1 - _a[u]) * _a[u];
    }
    bprop(u,g,r,m);
  }
}
