{
  long processed=_training.processed();
  float m=momentum(processed);
  float r=rate(processed) * (1 - m);
  float[] v=target();
  assert(params.loss == NeuralNet.Loss.MeanSquare);
  for (int u=0; u < _a.length; u++) {
    if (v[u] == missing_float_value)     continue;
    float g=v[u] - _a[u];
    g*=(1 - _a[u]) * _a[u];
    bprop(u,g,r,m);
  }
}
