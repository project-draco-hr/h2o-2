{
  Stats stats=new Stats();
  stats._units=_units;
  stats._categoricals=_categoricals;
  stats.doAll(_vecs);
  for (int i=0; i < vecs.length; i++) {
    _subs[i]=(float)stats._means[i];
    double sigma=Math.sqrt(stats._sigms[i] / (stats._rows - 1));
    _muls[i]=(float)(sigma > 1e-6 ? 1 / sigma : 1);
  }
}
