{
  for (int o=0; o < _a.length; o++) {
    _a[o]=0;
    if (!training || rand.nextFloat() > dropout) {
      for (int i=0; i < _previous._a.length; i++)       _a[o]+=_w[o * _previous._a.length + i] * _previous._a[i];
      _a[o]+=_b[o];
      if (_a[o] < 0)       _a[o]=0;
 else       if (!training)       _a[o]*=1 - _previous.dropout;
    }
  }
}
