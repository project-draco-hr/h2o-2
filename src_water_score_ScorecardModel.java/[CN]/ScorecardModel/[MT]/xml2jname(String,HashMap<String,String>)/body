{
  StringBuilder nn=new StringBuilder();
  char[] cs=xml.toCharArray();
  if (!Character.isJavaIdentifierStart(cs[0]))   nn.append('X');
  for (  char c : cs) {
    if (!Character.isJavaIdentifierPart(c)) {
      nn.append('_');
    }
 else {
      nn.append(c);
    }
  }
  String jname=nn.toString();
  if (vars.containsKey(jname)) {
    throw H2O.unimpl();
  }
  return jname;
}
