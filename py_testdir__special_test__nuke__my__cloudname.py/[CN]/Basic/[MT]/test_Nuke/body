def test_Nuke(self):
    h2o.build_cloud(1)
    print 'Waiting 10 secs for unknown number of possible zombies'
    time.sleep(10)
    c = h2o.nodes[0].get_cloud()
    cloudSize = c['cloud_size']
    print 'This node thought there was', cloudSize, 'nodes in its cloud'
    h2o.nodes[0].shutdown_all()
    h2o.tear_down_cloud()
