{
  super.logStart();
  if (cols == null) {
    Log.info("    cols: null");
  }
 else {
    Log.info("    cols: " + cols.length + " columns selected");
  }
  if (ignored_cols == null) {
    Log.info("    ignored_cols: null");
  }
 else {
    Log.info("    ignored_cols: " + ignored_cols.length + " columns ignored");
  }
}
