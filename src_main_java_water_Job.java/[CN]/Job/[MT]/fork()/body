{
  init();
  H2OCountedCompleter task=new H2OCountedCompleter(){
    @Override public void compute2(){
      Throwable t=null;
      try {
        Job.this.exec();
        Job.this.done();
      }
 catch (      Throwable t_) {
        t=t_;
        if (!(t instanceof ExpectedExceptionForDebug))         Log.err(t);
      }
 finally {
        tryComplete();
      }
      if (t != null)       update(Job.this,Utils.getStackAsString(t));
    }
  }
;
  start(task);
  H2O.submitTask(task);
  return this;
}
