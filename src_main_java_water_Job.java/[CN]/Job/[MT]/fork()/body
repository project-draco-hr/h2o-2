{
  init();
  H2OCountedCompleter task=new H2OCountedCompleter(){
    @Override public void compute2(){
      Throwable t=null;
      try {
        Status status=Job.this.exec();
        if (status == Status.Done)         Job.this.remove();
      }
 catch (      Throwable t_) {
        t=t_;
        if (!(t instanceof ExpectedExceptionForDebug))         Log.err(t);
      }
 finally {
        tryComplete();
      }
      if (t != null)       Job.this.cancel(t);
    }
  }
;
  start(task);
  H2O.submitTask(task);
  return this;
}
