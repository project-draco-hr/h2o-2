{
  DKV.put(_self,new Value(_self,""));
  new TAtomic<List>(){
    @Override public List atomic(    List old){
      Job[] jobs=old._jobs;
      old._jobs=new Job[jobs.length + 1];
      System.arraycopy(jobs,0,old._jobs,0,jobs.length);
      old._jobs[old._jobs.length - 1]=Job.this;
      return old;
    }
  }
.invoke(LIST);
}
