{
  Frame fr=UKV.get(Key.make(target));
  if (fr == null) {
    return false;
  }
  RString aft=new RString("<a href='Inspect2.html?src_key=%$key'>%key</a>");
  aft.replace("key",target);
  DocGen.HTML.section(sb,report() + "<br/>Frame '" + aft.toString()+ "' now has "+ fr.numCols()+ " columns.");
  return true;
}
