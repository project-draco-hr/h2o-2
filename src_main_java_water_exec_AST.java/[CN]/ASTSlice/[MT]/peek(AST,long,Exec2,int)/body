{
  if (ast._cols != 1)   E.throwErr("Slice selector can only be a single column",x);
  if (ast instanceof ASTNum) {
    long d=(long)((ASTNum)ast)._d;
    if (d < -len)     return len;
    if (d < 0)     return len - 1;
    if (d == 0)     return 0;
    if (d < len)     return 1;
    E.throwErr("Select off end: " + d,x);
  }
  throw H2O.unimpl();
}
