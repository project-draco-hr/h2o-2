{
  AST ast=parseExpr(E);
  if (ast == null)   return null;
  if (ast._t != Type.ary)   return ast;
  if (!E.peek('['))   return ast;
  if (E.peek(']'))   return ast;
  int x;
  AST rows=E.xpeek(',',(x=E._x),parseExpr(E));
  AST cols=E.xpeek(']',(x=E._x),parseExpr(E));
  return new ASTSlice(ast,cols,rows);
}
