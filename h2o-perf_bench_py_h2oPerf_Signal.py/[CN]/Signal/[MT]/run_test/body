@staticmethod
def run_test():
    '\n        A unit test for the Signal class.\n        '
    x = range(20)
    x2 = [0.123, 232.13, 21938, 2341, 214, 124, 214, 9291, 12313, 8237.123, 23, 14, 21, 3, 4, 6, 7, 8, 1293, 10191]
    s1 = Signal(5)
    s2 = Signal(1)
    for i in x[::(-1)]:
        s1.add(i)
        s1.report()
    for i in x2[::(-1)]:
        s2.add(i)
        s2.report()
