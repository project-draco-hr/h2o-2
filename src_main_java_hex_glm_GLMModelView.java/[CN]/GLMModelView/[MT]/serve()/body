{
  glm_model=DKV.get(_modelKey).get();
  if (Double.isNaN(lambda))   lambda=glm_model.lambdas[glm_model.best_lambda_idx];
  Job j;
  if (DKV.get(glm_model.job_key) != null && (j=Job.findJob(glm_model.job_key)) != null)   return Response.poll(this,(int)(100 * j.progress()),100,"_modelKey",_modelKey.toString());
 else   return Response.done(this);
}
