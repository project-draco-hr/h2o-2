{
  sb.append("<tr><th>").append(hd).append("</th><td>");
  if (!Double.isNaN(c0))   sb.append(DFORMAT.format(c0));
  sb.append("</td><td>");
  if (!Double.isNaN(c1))   sb.append(DFORMAT.format(c1));
  sb.append("</td><td>");
  if (!Double.isNaN(cerr))   sb.append(DFORMAT.format(cerr));
  sb.append("</td></tr>");
}
