def test_Cloud(self):
    for tryNodes in range(2, 8):
        h2o.verboseprint('Trying cloud of', tryNodes)
        sys.stdout.write('.')
        sys.stdout.flush()
        start = time.time()
        h2o.build_cloud(tryNodes)
        print ('Built cloud of %d in %d s' % (tryNodes, (time.time() - start)))
        h2o.verify_cloud_size()
        h2o.tear_down_cloud()
