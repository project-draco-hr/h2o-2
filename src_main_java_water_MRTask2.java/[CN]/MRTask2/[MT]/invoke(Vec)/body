{
  checkCompatible(firstReadable(vecs),vecs);
  _vecs=vecs;
  _nlo=0;
  _nhi=H2O.CLOUD.size();
  setupLocal();
  H2O.submitTask(this);
  try {
    get();
  }
 catch (  InterruptedException e) {
    Log.errRTExcept(e);
  }
catch (  ExecutionException e) {
    Log.errRTExcept(e);
  }
  return self();
}
