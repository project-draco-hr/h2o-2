{
  checkCompatible(firstReadable(vecs),vecs);
  _vecs=vecs;
  _nlo=0;
  _nhi=H2O.CLOUD.size();
  setupLocal();
  H2O.submitTask(this);
  try {
    get();
  }
 catch (  InterruptedException e) {
    Log.errRTExcept(e);
  }
catch (  ExecutionException e) {
    Log.errRTExcept(e);
  }
  for (int i=0; i < _vecs.length; i++) {
    Vec v=_vecs[i];
    if (v != null && v instanceof AppendableVec)     _vecs[i]=((AppendableVec)v).close();
  }
  return self();
}
