{
  reduce3(_nleft);
  reduce3(_nrite);
  _profile._remoteBlkDone=System.currentTimeMillis();
  if (_fs != null)   _fs.blockForPending();
  _profile._localBlkDone=System.currentTimeMillis();
  int nlo=_nlo, nhi=_nhi;
  if (_res == null)   _nlo=-1;
 else   if (_res != this) {
    _res._profile=_profile;
    copyOver(_res);
  }
  closeLocal();
  if (nlo == 0 && nhi == H2O.CLOUD.size())   _fr.closeAppendables();
}
