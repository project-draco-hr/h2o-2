        plt.xlabel('max_depth')
        plt.ylabel('pctWrong')
        plt.title(label)
        plt.draw()
        label = '1jvmx28GB Covtype GBM learn_rate=.2 nbins=1024 ntrees=10 min_rows = 10'
        plt.figure()
        plt.plot(xList, sList)
        plt.xlabel('max_depth')
        plt.ylabel('time')
        plt.title(label)
        plt.draw()
        plt.show()
