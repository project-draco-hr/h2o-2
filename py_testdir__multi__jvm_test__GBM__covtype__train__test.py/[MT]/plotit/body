def plotit(xList, eList, sList):
    if (h2o.python_username != 'kevin'):
        return
    import pylab as plt
    if eList:
        print 'xList', xList
        print 'eList', eList
        print 'sList', sList
        font = {'family': 'normal', 'weight': 'normal', 'size': 26, }
        plt.rcdefaults()
        label = '1jvmx28GB covtype train 90/test 10 GBM learn_rate=.2 nbins=1024 ntrees=10 min_rows = 10'
        plt.figure()
