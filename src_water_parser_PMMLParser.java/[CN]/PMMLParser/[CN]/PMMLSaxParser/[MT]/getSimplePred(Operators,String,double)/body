{
switch (op) {
case lessOrEqual:
    return new ScorecardModel.LessOrEqual(value,d);
case lessThan:
  return new ScorecardModel.LessThan(value,d);
case greaterOrEqual:
return new ScorecardModel.GreaterOrEqual(value,d);
case greaterThan:
return new ScorecardModel.GreaterThan(value,d);
case equal:
return new ScorecardModel.Equals(value,d);
default :
return null;
}
}
