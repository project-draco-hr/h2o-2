{
  DeepLearning req=new DeepLearning();
  RString rs=new RString("<a href='" + req.href() + ".query?source=%$key"+ (cp == null ? "" : "&checkpoint=%$cp")+ (response == null ? "" : "&response=%$resp")+ "'>%content</a>");
  rs.replace("key",k.toString());
  rs.replace("content",content);
  if (cp != null)   rs.replace("cp",cp.toString());
  if (response != null)   rs.replace("resp",response);
  return rs.toString();
}
