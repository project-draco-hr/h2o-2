{
  super.registered(ver);
  for (  Argument arg : _arguments) {
    if (arg._name.equals("activation") || arg._name.equals("initial_weight_distribution") || arg._name.equals("expert_mode")|| arg._name.equals("adaptive_rate")|| arg._name.equals("replicate_training_data")|| arg._name.equals("balance_classes")|| arg._name.equals("checkpoint")) {
      arg.setRefreshOnChange();
    }
  }
}
