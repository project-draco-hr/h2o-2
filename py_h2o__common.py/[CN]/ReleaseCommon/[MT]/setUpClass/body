@classmethod
def setUpClass(cls):
    print 'setUpClass'
    h2o.setup_random_seed()
    if h2o.config_json:
        h2o_hosts.build_cloud_with_hosts()
    else:
        h2o.build_cloud_with_json()
    h2o.stabilize_cloud(h2o.nodes[0], node_count=len(h2o.nodes), timeoutSecs=90)
    h2o.verify_cloud_size()
