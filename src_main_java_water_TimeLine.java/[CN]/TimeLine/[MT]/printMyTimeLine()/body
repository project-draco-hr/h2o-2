{
  long[] s=TimeLine.snapshot();
  System.out.println("===================================<TIMELINE>==============================================");
  for (int i=0; i < TimeLine.length(); ++i) {
    if (!TimeLine.isEmpty(s,i) && ((TimeLine.l0(s,i) & 0xFF) == UDP.udp.exec.ordinal()))     System.out.println(TimeLine.ms(s,i) + ": " + (((TimeLine.ns(s,i) & 4) != 0) ? "TCP" : "UDP")+ TimeLine.inet(s,i)+ " | "+ UDP.printx16(TimeLine.l0(s,i),TimeLine.l8(s,i)));
  }
  System.out.println("===========================================================================================");
}
