{
  if (_ok[i] < H2O.CLOUD.size())   return null;
  File f=new File(_files[i]);
  Key k=PersistNFS.decodeFile(f);
  long size=f.length();
  Value val=(size < 2 * ValueArray.CHUNK_SZ) ? new Value(k,(int)size,Value.NFS) : new ValueArray(k,size,Value.NFS).value();
  val.setdsk();
  if (fs == null)   UKV.put(k,val);
 else   UKV.put(k,val,fs);
  return k;
}
