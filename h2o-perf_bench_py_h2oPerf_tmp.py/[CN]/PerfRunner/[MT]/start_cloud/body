def start_cloud(self):
    '\n        Start H2O Cloud\n        '
    if self.terminated:
        return
    if self.use_cloud:
        return
    print ''
    print 'Starting cloud...'
    print ''
    if self.terminated:
        return
    self.cloud.start()
    print ''
    print 'Waiting for H2O nodes to come up...'
    print ''
    if self.terminated:
        return
    self.cloud.wait_for_cloud_to_be_up()
    self.jvm_output_file = self.cloud.nodes[0].get_output_file_name()
