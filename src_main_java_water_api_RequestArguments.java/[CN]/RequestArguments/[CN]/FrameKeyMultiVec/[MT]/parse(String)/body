{
  Frame fr=fr();
  ArrayList<Integer> al=new ArrayList();
  for (  String col : input.split(",")) {
    col=col.trim();
    int idx=index(col);
    if (0 > idx || idx > fr.numCols())     throw new IllegalArgumentException("Column " + col + " not part of key "+ _key.value());
    if (al.contains(idx))     throw new IllegalArgumentException("Column " + col + " is specified twice.");
    checkLegality(fr.vecs()[idx]);
    al.add(idx);
  }
  return Ints.toArray(al);
}
