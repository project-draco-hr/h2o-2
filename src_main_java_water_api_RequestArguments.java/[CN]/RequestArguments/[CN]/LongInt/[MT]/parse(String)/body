{
  try {
    long i=Long.parseLong(input);
    if ((i < _min) || (i > _max))     throw new IllegalArgumentException(_name + "Value " + i+ " is not between "+ _min+ " and "+ _max+ " (inclusive)");
    return i;
  }
 catch (  NumberFormatException e) {
    throw new IllegalArgumentException(_name + "Value " + input+ " is not a valid long integer.");
  }
}
