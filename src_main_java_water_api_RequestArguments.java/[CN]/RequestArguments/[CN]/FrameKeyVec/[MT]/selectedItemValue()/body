{
  Frame fr=fr();
  if (value() == null || fr == null) {
    if (!refreshOnChange()) {
      Vec defaultVec=defaultValue();
      if (defaultVec != null && fr != null)       for (int i=0; i < fr.vecs().length; i++)       if (fr.vecs()[i] == defaultVec)       return fr._names[i];
    }
    return "";
  }
  return fr._names[_colIdx.get()];
}
