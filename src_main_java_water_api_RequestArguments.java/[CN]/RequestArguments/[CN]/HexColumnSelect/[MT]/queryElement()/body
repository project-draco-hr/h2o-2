{
  ValueArray va=_key.value();
  ArrayList<Integer> cols=Lists.newArrayList();
  for (int i=0; i < Math.min(5000,va._cols.length); ++i)   if (!shouldIgnore(i,va._cols[i]))   cols.add(i);
  Comparator<Integer> cmp=colComp(va);
  if (cmp != null)   Collections.sort(cols,cmp);
  _selectedCols=cols;
  return super.queryElement();
}
