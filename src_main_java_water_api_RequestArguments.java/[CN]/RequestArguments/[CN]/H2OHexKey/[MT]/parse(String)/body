{
  Key k=Key.make(input);
  Value v=DKV.get(k);
  if (v == null)   throw new IllegalArgumentException("Key " + input + " not found!");
  if (v.isFrame())   throw new IllegalArgumentException("Key " + input + " represents fluid vector frame! Please use manual transformation to switch frame to value array.");
  if (!v.isArray())   throw new IllegalArgumentException("Key " + input + " is not a valid HEX key");
  return v.get();
}
