{
  final Frame fr=new Frame(source);
  if (!vresponse.isEnum())   vresponse.asEnum();
  String vname="response";
  for (int i=0; i < fr.numCols(); i++)   if (fr._vecs[i] == vresponse) {
    vname=fr._names[i];
    fr.remove(i);
  }
  buildModel(fr,vname);
  return DRFProgressPage.redirect(this,self(),dest());
}
