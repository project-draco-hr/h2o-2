def parseFile(node=None, csvPathname=None, key=None, key2=None, timeoutSecs=30, retryDelaySecs=0.5, pollTimeoutSecs=30, noise=None, noPoll=None, doSummary=True, **kwargs):
    if (not csvPathname):
        raise Exception('No file name specified')
    if (not node):
        node = h2o.nodes[0]
    key = node.put_file(csvPathname, key=key, timeoutSecs=timeoutSecs)
    if (key2 is None):
        myKey2 = (key + '.hex')
    else:
        myKey2 = key2
    p = node.parse(key, myKey2, timeoutSecs, retryDelaySecs, pollTimeoutSecs=pollTimeoutSecs, noise=noise, noPoll=noPoll, **kwargs)
    if doSummary:
        node.summary_page(myKey2, timeoutSecs=timeoutSecs)
    return p
