def parseFile(node=None, csvPathname=None, key=None, key2=None, timeoutSecs=20, retryDelaySecs=0.5, noise=None):
    if (not csvPathname):
        raise Exception('No file name specified')
    if (not node):
        node = h2o.nodes[0]
    key = node.put_file(csvPathname, key=key, timeoutSecs=timeoutSecs)
    if (key2 is None):
        myKey2 = (key + '.hex')
    else:
        myKey2 = key2
    return node.parse(key, myKey2, timeoutSecs=timeoutSecs, retryDelaySecs=retryDelaySecs, noise=noise)
