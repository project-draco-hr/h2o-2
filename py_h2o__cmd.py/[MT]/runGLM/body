def runGLM(node=None, parseResult=None, timeoutSecs=20, retryDelaySecs=2, noise=None, noPoll=False, **kwargs):
    if (not parseResult):
        raise Exception('No parseResult for GLM')
    if (not node):
        node = h2o.nodes[0]
    return node.GLM(parseResult['destination_key'], timeoutSecs, retryDelaySecs, noise=noise, noPoll=noPoll, **kwargs)
