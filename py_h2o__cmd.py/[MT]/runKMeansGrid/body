def runKMeansGrid(node=None, parseResult=None, timeoutSecs=60, retryDelaySecs=2, noise=None, **kwargs):
    if (not parseResult):
        raise Exception('No parseResult for KMeansGrid')
    if (not node):
        node = h2o.nodes[0]
    return node.kmeans_grid(parseResult['destination_key'], timeoutSecs, **kwargs)
