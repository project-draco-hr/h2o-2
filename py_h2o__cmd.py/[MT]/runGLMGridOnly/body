def runGLMGridOnly(node=None, parseKey=None, timeoutSecs=60, retryDelaySecs=2, noise=None, **kwargs):
    if (not parseKey):
        raise Exception('No parsed key for GLMGrid specified')
    if (not node):
        node = h2o.nodes[0]
    return node.GLMGrid(parseKey['destination_key'], timeoutSecs, **kwargs)
