{
  StringBuilder sb=new StringBuilder();
  sb.append("<table class='table table-bordered table-condensed'>");
  sb.append("<tr>");
  for (  Entry<String,JsonElement> e : coefs.entrySet())   sb.append("<th>").append(e.getKey()).append("</th>");
  sb.append("</tr>");
  sb.append("<tr>");
  for (  Entry<String,JsonElement> e : coefs.entrySet())   sb.append("<td>").append(e.getValue().getAsDouble()).append("</td>");
  sb.append("</tr>");
  sb.append("</table>");
  return sb.toString();
}
