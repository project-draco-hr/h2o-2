{
  Layer[] ls=new Layer[5];
  ls[0]=new VecsInput(data,inputStats);
  for (int i=1; i < ls.length - 1; i++)   ls[i]=new Tanh(1000);
  ls[ls.length - 1]=new VecSoftmax(labels,outputStats);
  for (int i=0; i < ls.length; i++) {
    ls[i].rate=.005f;
    ls[i].rate_annealing=1 / 1e6f;
    ls[i].l2=.001f;
    ls[i].init(ls,i);
  }
  return ls;
}
