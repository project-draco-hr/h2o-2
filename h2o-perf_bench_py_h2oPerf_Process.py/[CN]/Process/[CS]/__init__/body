def __init__(self, test_dir, test_short_dir, output_dir):
    self.test_dir = test_dir
    self.test_short_dir = test_short_dir
    self.output_dir = output_dir
    self.test_name = ''
    self.output_file_name = ''
    self.canceled = False
    self.terminated = False
    self.returncode = None
    self.ip = None
    self.pid = (-1)
    self.port = None
    self.port = None
    self.child = None
