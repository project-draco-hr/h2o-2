{
  if (repl >= size())   return -1;
  byte[] kb=key._kb;
  if (!key.user_allowed() && repl < kb[1]) {
    H2ONode h2o=null, h2otmp=new H2ONode();
    AutoBuffer ab=new AutoBuffer(kb,2);
    for (int i=0; i <= repl; i++)     h2o=h2otmp.read(ab);
    int idx=nidx(h2o);
    if (idx != -1)     return idx;
  }
  return ((key._hash + repl) & 0x7FFFFFFF) % size();
}
