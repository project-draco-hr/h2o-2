{
  KMeans task=(KMeans)rt;
  if (_sums == null) {
    _sums=task._sums;
    _counts=task._counts;
    _error=task._error;
  }
 else {
    for (int cluster=0; cluster < _counts.length; cluster++) {
      for (int column=0; column < _sums[cluster].length; column++)       _sums[cluster][column]+=task._sums[cluster][column];
      _counts[cluster]+=task._counts[cluster];
    }
    _error+=task._error;
  }
}
