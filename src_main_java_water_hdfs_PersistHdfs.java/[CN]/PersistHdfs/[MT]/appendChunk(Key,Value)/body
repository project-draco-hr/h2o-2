{
  String res=null;
  FSDataOutputStream s=null;
  try {
    Path p=getPathForKey(key);
    FileSystem fs=FileSystem.get(p.toUri(),CONF);
    s=fs.append(p);
    Log.info(Sys.HDFS_,"append=" + val.memOrLoad().length);
    s.write(val.memOrLoad());
  }
 catch (  IOException e) {
    Log.err(e);
    res=e.getMessage();
  }
 finally {
    try {
      if (s != null)       s.close();
    }
 catch (    IOException e) {
      Log.err(e);
    }
  }
  return res;
}
