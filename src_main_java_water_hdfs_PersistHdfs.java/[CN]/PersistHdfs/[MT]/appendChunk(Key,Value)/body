{
  String res=null;
  FSDataOutputStream s=null;
  try {
    Path p=getPathForKey(key);
    FileSystem fs=FileSystem.get(p.toUri(),CONF);
    s=fs.append(p);
    System.err.println("[hdfs] append=" + val.get().length);
    s.write(val.get());
  }
 catch (  IOException e) {
    res=e.getMessage();
  }
 finally {
    Closeables.closeQuietly(s);
  }
  return res;
}
