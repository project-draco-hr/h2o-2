{
  testImpl(new long[]{122,3,44},new int[]{0,0,0},C1Vector.class);
  testImpl(new long[]{122,-3,44},new int[]{-1,0,-1},C1SVector.class);
  testImpl(new long[]{1000,200,30},new int[]{0,1,2},C1SVector.class);
  testImpl(new long[]{1000,200,32767,-32767,32},new int[]{0,1,0,0,3},C2Vector.class);
  testImpl(new long[]{50100,50101,50123,49999},new int[]{0,0,0,0},C1SVector.class);
  testImpl(new long[]{51000,50101,50123,49999},new int[]{0,0,0,0},C2SVector.class);
  testImpl(new long[]{501000,501001,50123,49999},new int[]{-1,-1,0,0},C2SVector.class);
  testImpl(new long[]{123456,2345678,34567890},new int[]{0,0,0},C4Vector.class);
  testImpl(new long[]{1234,2345678,31415},new int[]{20,10,-4},C4FVector.class);
  testImpl(new long[]{1234,2345678,31415},new int[]{40,10,-40},C8DVector.class);
}
