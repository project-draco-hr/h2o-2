{
  if (cm != null) {
    long[][] m1=_matrix;
    long[][] m2=cm._matrix;
    if (m1 == null)     _matrix=m2;
 else {
      for (int i=0; i < m1.length; i++)       for (int j=0; j < m1.length; j++)       m1[i][j]+=m2[i][j];
    }
    _rows+=cm._rows;
    _errors+=cm._errors;
    _skippedRows+=cm._skippedRows;
  }
  return this;
}
