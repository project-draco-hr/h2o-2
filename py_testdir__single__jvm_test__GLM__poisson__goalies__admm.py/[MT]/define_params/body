def define_params():
    print 'Always using standardize=1. Unable to solve sometimes if not?'
    paramDict = {'family': ['poisson'], 'n_folds': [1], 'thresholds': [0.5], 'lambda': [0, 1e-08], 'alpha': [0], 'beta_epsilon': [0.001, 0.0001], 'max_iter': [3], 'weight': [None, 1, 2, 4], 'expert': [None, 0, 1], 'lsm_solver': ['ADMM'], 'standardize': [1], }
    return paramDict
