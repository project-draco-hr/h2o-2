{
  String[] data=new String[]{"1|2|3\n1|2|3","4|5|6","4|5.2|","asdf|qwer|1","1.1","1.1|2.1|3.4"};
  double[][] exp=new double[][]{d(1.0,2.0,3.0),d(1.0,2.0,3.0),d(4.0,5.0,6.0),d(4.0,5.2,NaN),d(NaN,NaN,1.0),d(1.1,NaN,NaN),d(1.1,2.1,3.4)};
  for (  char separator : SEPARATORS) {
    String[] dataset=getDataForSeparator(separator,data);
    StringBuilder sb=new StringBuilder();
    for (int i=0; i < dataset.length; ++i)     sb.append(dataset[i]).append("\n");
    Key k=Key.make();
    UKV.put(k,new Value(k,sb.toString()));
    Key r1=Key.make("r1");
    ParseDataset.parse(r1,DKV.get(k));
    testParsed(r1,exp,k);
    sb=new StringBuilder();
    for (int i=0; i < dataset.length; ++i)     sb.append(dataset[i]).append("\r\n");
    UKV.put(k,new Value(k,sb.toString()));
    Key r2=Key.make("r2");
    ParseDataset.parse(r2,DKV.get(k));
    testParsed(r2,exp,k);
  }
}
