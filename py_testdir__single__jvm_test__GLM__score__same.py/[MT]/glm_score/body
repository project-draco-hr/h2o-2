def glm_score(self, csvFilename, bucket, csvPathname, modelKey, thresholds='0.5', timeoutSecs=30, pollTimeoutSecs=30):
    print '\nStarting GLM score of', csvFilename
    hex_key = (csvFilename + '.hex')
    parseResult = h2i.import_parse(bucket=bucket, path=csvPathname, hex_key=hex_key, timeoutSecs=timeoutSecs, pollTimeoutSecs=pollTimeoutSecs)
    y = '10'
    x = ''
    kwargs = {'x': x, 'y': y, 'case': (-1), 'thresholds': 0.5, }
    start = time.time()
    glmScore = h2o_cmd.runGLMScore(key=hex_key, model_key=modelKey, thresholds='0.5', timeoutSecs=timeoutSecs)
    print 'GLMScore in', (time.time() - start), 'secs (python)'
    h2o.verboseprint(h2o.dump_json(glmScore))
    validation = glmScore['validation']
    if self.validations1:
        h2o_glm.compareToFirstGlm(self, 'err', validation, self.validations1)
    else:
        self.validations1 = copy.deepcopy(validation)
