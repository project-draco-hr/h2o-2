{
  JsonObject res=new JsonObject();
  Key k=ServletUtil.check_key(p,"Key");
  Value v=DKV.get(k);
  if (v == null) {
    res.addProperty("error","no such key in K/V store!");
    return res;
  }
  if (v._isArray == 0) {
    byte[] mem=v.get();
    int r=0;
    for (    byte b : mem)     r^=b;
    res.addProperty("result",r);
    return res;
  }
  NOPTask tsk=new NOPTask();
  tsk.invoke(k);
  res.addProperty("result",tsk.toString());
  return res;
}
