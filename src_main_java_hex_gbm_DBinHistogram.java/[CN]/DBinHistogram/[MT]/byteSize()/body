{
  long sum=super.byteSize();
  sum+=8 + byteSize(_bins);
  sum+=8 + byteSize(_mins);
  sum+=8 + byteSize(_maxs);
  sum+=8 + byteSize(_Ms);
  if (_Ms != null)   for (int i=0; i < _Ms.length; i++)   sum+=byteSize(_Ms[i]);
  if (_Ss != null)   for (int i=0; i < _Ss.length; i++)   sum+=byteSize(_Ss[i]);
  return sum;
}
