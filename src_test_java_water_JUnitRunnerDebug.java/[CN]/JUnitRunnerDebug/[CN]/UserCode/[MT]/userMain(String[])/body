{
  H2O.main(args);
  TestUtil.stall_till_cloudsize(NODES);
  List<Class> tests=new ArrayList<Class>();
  tests.add(NeuralNetSpiralsTest.class);
  tests.add(NeuralNetIrisTest.class);
  tests.add(NeuralNetSpiralsTest2.class);
  tests.add(NeuralNetIrisTest2.class);
  tests.add(Dropout.class);
  JUnitCore junit=new JUnitCore();
  junit.addListener(new LogListener());
  Result result=junit.run(tests.toArray(new Class[0]));
  if (result.getFailures().size() == 0) {
    Log.info("SUCCESS!");
    System.exit(0);
  }
 else {
    Log.info("FAIL!");
    System.exit(1);
  }
}
