def test_hdfs_fvec(self):
    h2o.beta_features = True
    print '\nLoad a list of files from HDFS, parse and do 1 RF tree'
    print '\nYou can try running as hduser/hduser if fail'
    csvFilenameAll = ['3G_poker_shuffle', 'airlines_all.csv', 'and-testing.data', 'arcene2_train.both', 'arcene_train.both', 'bestbuy_test.csv', 'covtype4x.shuffle.data', 'four_billion_rows.csv', 'hhp.unbalanced.012.data.gz', 'hhp.unbalanced.data.gz', 'leads.csv', 'prostate_long_1G.csv']
    if (1 == 0):
        csvFilenameList = random.sample(csvFilenameAll, 8)
    else:
        csvFilenameList = csvFilenameAll
    for csvFilename in csvFilenameList:
        print 'Loading', csvFilename, 'from HDFS'
        csvPathname = ('datasets/' + csvFilename)
        parseResult = h2i.import_parse(path=csvPathname, schema='hdfs', timeoutSecs=1000)
        print 'parse result:', parseResult['destination_key']
