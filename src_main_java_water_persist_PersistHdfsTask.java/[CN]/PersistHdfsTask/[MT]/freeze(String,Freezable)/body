{
  String res=null;
  FSDataOutputStream s=null;
  try {
    Path p=new Path(path);
    FileSystem fs=FileSystem.get(p.toUri(),PersistHdfs.CONF);
    fs.mkdirs(p.getParent());
    s=fs.create(p);
    byte[] b=f.write(new AutoBuffer()).buf();
    s.write(b);
  }
 catch (  IOException e) {
    res=e.getMessage();
  }
 finally {
    try {
      if (s != null)       s.close();
    }
 catch (    IOException e) {
    }
  }
  return res;
}
