{
  ValueArray ary=DKV.get(_arykey).get();
  Key self=selfKey();
  while (_indexFrom < ary.chunks()) {
    Key ckey=ary.getChunkKey(_indexFrom++);
    if (!ckey.home()) {
      RPC.call(chunkHome(),this);
      return;
    }
    Value val=DKV.get(ckey);
    _err=appendChunk(_path,val.memOrLoad());
    if (_err != null)     return;
    UKV.put(self,this);
  }
  UKV.remove(self);
}
