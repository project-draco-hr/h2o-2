{
  UKV.put(destination_key,this);
  int max=jobs[0].gridParallelism();
  int head=0, tail=0;
  while (head < jobs.length && !cancelled()) {
    if (tail - head < max && tail < jobs.length)     jobs[tail++].fork();
 else {
      try {
        jobs[head++].get();
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
  }
}
