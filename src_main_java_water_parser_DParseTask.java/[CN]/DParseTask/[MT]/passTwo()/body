if (_ab != null) _outputStreams2[_outputIdx].store();
break;
default :
throw new Error("NOT IMPLEMENTED");
}
for (int i=0; i < _ncolumns; ++i) _sigma[i]=Math.sqrt(_sigma[i] / (_numRows - _invalidValues[i]));
createValueArrayHeader();
