{
  _phase=Pass.TWO;
  _myrows=0;
  if (DKV.get(_job.dest()) != null)   UKV.remove(_job.dest());
  if (_parser.parallelParseSupported())   this.invoke(_sourceDataset._key);
 else {
    phaseTwoInitialize();
    _outputStreams2=createRecords(0,(int)_numRows);
    assert(_outputStreams2.length > 0);
    _ab=_outputStreams2[0].initialize();
    try {
      _streamMode=true;
      _parser.streamParse(DKV.get(_sourceDataset._key).openStream(),this);
    }
 catch (    Exception e) {
      e.printStackTrace();
      throw new RuntimeException(e);
    }
    if (_ab != null)     _outputStreams2[_outputIdx].store();
  }
  normalizeSigma();
}
