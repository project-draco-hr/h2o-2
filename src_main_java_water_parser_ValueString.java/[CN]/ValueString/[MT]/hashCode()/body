{
  int hash=0;
  if (_skips.isEmpty()) {
    int n=_off + _length;
    for (int i=_off; i < n; ++i)     hash=31 * hash + _buf[i];
    return hash;
  }
 else {
    int i=_off;
    for (    int n : _skips) {
      for (; i < _off + n; ++i)       hash=31 * hash + _buf[i];
      ++i;
    }
    int n=_off + _length;
    for (; i < n; ++i)     hash=31 * hash + _buf[i];
    return hash;
  }
}
