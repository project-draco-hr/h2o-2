{
  if (t._nobs != 0) {
    if (_nobs == 0) {
      _ymu=t._ymu;
      _nobs=t._nobs;
      _ymin=t._ymin;
      _ymax=t._ymax;
    }
 else {
      _ymu=_ymu * ((double)_nobs / (_nobs + t._nobs)) + t._ymu * t._nobs / (_nobs + t._nobs);
      _nobs+=t._nobs;
      _ymax=Math.max(_ymax,t._ymax);
      _ymin=Math.min(_ymin,t._ymin);
    }
  }
}
