{
  if (submodels != null)   if (submodels[0].xvalidation != null && (submodels[0].xvalidation instanceof GLMXValidation)) {
    GLMXValidation xval=(GLMXValidation)submodels[0].xvalidation;
    for (    Key k : xval.xval_models) {
      GLMModel m=DKV.get(k).get();
      m.unlock(job_key);
    }
  }
}
