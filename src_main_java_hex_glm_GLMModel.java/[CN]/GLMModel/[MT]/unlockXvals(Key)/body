{
  if (submodels != null)   for (  Submodel sm : submodels)   if (sm.xvalidation != null && (sm.xvalidation instanceof GLMXValidation)) {
    GLMXValidation xval=(GLMXValidation)sm.xvalidation;
    for (    Key k : xval.xval_models) {
      GLMModel m=DKV.get(k).get();
      m.unlock(job_key);
    }
  }
}
