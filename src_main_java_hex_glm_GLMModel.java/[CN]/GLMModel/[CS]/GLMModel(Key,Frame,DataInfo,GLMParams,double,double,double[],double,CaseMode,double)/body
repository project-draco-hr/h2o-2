{
  super(selfKey,null,fr);
  this.ymu=ymu;
  this.glm=glm;
  threshold=0.5;
  this.catOffsets=dinfo._catOffsets;
  this.warnings=null;
  this.alpha=alpha;
  this.beta_eps=beta_eps;
  _caseVal=caseVal;
  _caseMode=caseMode;
  submodels=new Submodel[lambda.length];
  for (int i=0; i < submodels.length; ++i)   submodels[i]=new Submodel(lambda[i],null,null,0,0);
  run_time=0;
  start_time=System.currentTimeMillis();
  coefficients_names=coefNames(dinfo.fullN() + 1);
}
