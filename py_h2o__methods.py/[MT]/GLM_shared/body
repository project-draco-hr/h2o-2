def GLM_shared(self, key, timeoutSecs=300, retryDelaySecs=0.5, initialDelaySecs=None, pollTimeoutSecs=180, parentName=None, **kwargs):
    browseAlso = kwargs.pop('browseAlso', False)
    params_dict = {'alpha': None, 'beta_epsilon': None, 'cols': None, 'destination_key': None, 'family': None, 'intercept': None, 'higher_accuracy': None, 'ignored_cols': None, 'ignored_cols_by_name': None, 'lambda': None, 'lambda_min_ratio': None, 'lambda_search': None, 'link': None, 'max_iter': None, 'max_predictors': None, 'n_folds': None, 'nlambdas': None, 'non_negative': None, 'prior': None, 'response': None, 'source': key, 'standardize': None, 'strong_rules': None, 'tweedie_variance_power': None, 'use_all_factor_levels': None, 'variable_importances': None, }
    check_params_update_kwargs(params_dict, kwargs, parentName, print_params=True)
    a = self.do_json_request((parentName + '.json'), timeout=timeoutSecs, params=params_dict)
    verboseprint(parentName, dump_json(a))
    return a
