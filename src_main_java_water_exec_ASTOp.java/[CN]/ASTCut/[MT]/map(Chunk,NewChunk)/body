{
  for (int r=0; r < chk._len; r++) {
    double x=chk.at0(r);
    if (x <= cutoffs[0] || x > cutoffs[cutoffs.length - 1])     nchk.addNum(Double.NaN);
 else {
      for (int i=1; i < cutoffs.length; i++) {
        if (x <= cutoffs[i]) {
          nchk.addNum(i - 1);
          break;
        }
      }
    }
  }
}
