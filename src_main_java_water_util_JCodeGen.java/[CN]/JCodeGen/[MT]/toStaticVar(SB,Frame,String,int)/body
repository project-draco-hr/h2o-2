{
  sb.indent().p("public static final double[][] ").p(varname).p(" = new double[][] {").nl();
  if (f != null) {
    Vec[] vecs=f.vecs();
    for (int row=0; row < nrows; row++) {
      sb.indent(1).p(row > 0 ? "," : "").p("new double[] {");
      for (int v=0; v < vecs.length; v++)       sb.p(v > 0 ? "," : "").p(vecs[v].at(row));
      sb.p("}").nl();
    }
  }
  sb.indent().p("};").nl();
  return sb;
}
