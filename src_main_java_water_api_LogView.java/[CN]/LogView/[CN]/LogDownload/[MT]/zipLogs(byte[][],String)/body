{
  int l=0;
  for (int i=0; i < results.length; l+=results[i++].length)   ;
  ByteArrayOutputStream baos=new ByteArrayOutputStream(l);
  ZipOutputStream zos=new ZipOutputStream(baos);
{
    ZipEntry zde=new ZipEntry(topDir + File.separator);
    zos.putNextEntry(zde);
  }
  try {
    for (int i=0; i < results.length; i++) {
      String filename=topDir + File.separator + "node"+ i+ H2O.CLOUD._memary[i].toString().replace(':','_').replace('/','_')+ ".zip";
      ZipEntry ze=new ZipEntry(filename);
      zos.putNextEntry(ze);
      zos.write(results[i]);
      zos.closeEntry();
    }
    zos.closeEntry();
  }
  finally {
    zos.close();
  }
  return baos.toByteArray();
}
