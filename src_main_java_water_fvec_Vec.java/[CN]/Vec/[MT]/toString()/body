{
  String s="[" + length() + (_rs == null ? "" : "," + _rs._min + "/"+ _rs._mean+ "/"+ _rs._max+ ", "+ PrettyPrint.bytes(_rs._size)+ ", {");
  int nc=nChunks();
  for (int i=0; i < nc; i++) {
    s+=chunkKey(i).home_node() + ":" + chunk2StartElem(i)+ ":";
    s+=elem2BV(i).getClass().getSimpleName().replaceAll("Chunk","") + ", ";
  }
  return s + "}]";
}
