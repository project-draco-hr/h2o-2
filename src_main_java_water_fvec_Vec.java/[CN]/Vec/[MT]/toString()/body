{
  String s="[" + length() + (_naCnt < 0 ? ", {" : "," + _min + "/"+ _mean+ "/"+ _max+ ", "+ PrettyPrint.bytes(_size)+ ", {");
  int nc=nChunks();
  for (int i=0; i < nc; i++) {
    s+=chunkKey(i).home_node() + ":" + chunk2StartElem(i)+ ":";
    s+=elem2BV(i).getClass().getSimpleName().replaceAll("Chunk","") + ", ";
  }
  return s + "}]";
}
