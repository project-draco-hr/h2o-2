{
  final long now=_timestamp;
  if (now == _hash_timestamp)   return _hash;
  final long hash=new HasherTask().doAll(this).getHash();
  new TAtomic<Vec>(){
    @Override public Vec atomic(    Vec v){
      if (v != null)       v._hash=hash;
      v._hash_timestamp=now;
      return v;
    }
  }
.invoke(_key);
  return _hash;
}
