{
  System.out.println("Running ParserTest2");
  final int nnodes=1;
  for (int i=1; i < nnodes; i++) {
    Node n=new NodeVM(args);
    n.inheritIO();
    n.start();
  }
  H2O.waitForCloudSize(nnodes);
  System.out.println("Cloud formed");
  new ParserTest2().testSVMLight1();
  System.out.println("DONE!");
}
