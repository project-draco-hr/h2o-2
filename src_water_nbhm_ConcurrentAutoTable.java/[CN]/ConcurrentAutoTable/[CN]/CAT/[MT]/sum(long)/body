{
  long sum=_sum_cache;
  if (sum != Long.MIN_VALUE)   return sum;
  sum=_next == null ? 0 : _next.sum(mask);
  final long old_sum=sum;
  final long[] t=_t;
  for (int i=0; i < t.length; i++)   sum+=t[i] & (~mask);
  _sum_cache=sum;
  long sum2=old_sum;
  for (int i=0; i < t.length; i++)   sum2+=t[i] & (~mask);
  if (sum != sum2)   _sum_cache=Long.MIN_VALUE;
  return sum;
}
