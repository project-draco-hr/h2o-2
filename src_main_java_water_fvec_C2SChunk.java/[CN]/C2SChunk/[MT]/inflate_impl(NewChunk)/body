{
  double dx=Math.log10(_scale);
  int x=(int)dx;
  if (DParseTask.pow10i(x) != _scale)   throw H2O.unimpl();
  for (int i=0; i < _len; i++) {
    long res=UDP.get2(_mem,(i << 1) + OFF);
    if (res == _NA)     nc.setInvalid(i);
 else {
      nc._ls[i]=res + _bias;
      nc._xs[i]=x;
    }
  }
  return nc;
}
