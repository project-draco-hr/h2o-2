{
  StringBuilder res=new StringBuilder("ColumnSummary[" + start + ":"+ binValue(bins.length)+ ", binsz="+ binsz+ "]");
  if (!_enum)   for (int i=0; i < DEFAULT_PERCENTILES.length; i++)   res.append(", p(" + (int)(100 * DEFAULT_PERCENTILES[i]) + "%)="+ percentileValue(i));
  return res.toString();
}
