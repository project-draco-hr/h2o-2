{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < weights.length; ++i)   sb.append("\nweights[" + i + "][]="+ Arrays.toString(weights[i]));
  for (int i=0; i < biases.length; ++i)   sb.append("\nbiases[" + i + "][]="+ Arrays.toString(biases[i]));
  sb.append("\n");
  return sb.toString();
}
