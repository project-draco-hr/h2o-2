{
  Vec vecs[]=fr.vecs();
  for (int c=0; c < ncols; c++) {
    Vec v=vecs[c];
    final float minIn=(float)v.min();
    final float maxIn=(float)v.max();
    final float maxEx=find_maxEx(maxIn,v.isInt() ? 1 : 0);
    final long vlen=v.length();
    hs[c]=v.naCnt() == vlen || v.min() == v.max() || Float.isInfinite(minIn) || Float.isInfinite(maxIn) ? null : make(fr._names[c],nbins,(byte)(v.isEnum() ? 2 : (v.isInt() ? 1 : 0)),minIn,maxEx,vlen,doGrpSplit,isBinom);
  }
  return hs;
}
