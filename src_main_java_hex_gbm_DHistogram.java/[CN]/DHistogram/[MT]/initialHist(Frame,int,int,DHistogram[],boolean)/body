{
  Vec vecs[]=fr.vecs();
  for (int c=0; c < ncols; c++) {
    Vec v=vecs[c];
    float maxIn=(float)v.max();
    float maxEx=v.isInt() ? maxIn + 1 : maxIn + Math.ulp(maxIn);
    hs[c]=v.naCnt() == v.length() || v.min() == v.max() ? null : make(fr._names[c],nbins,(byte)(v.isEnum() ? 2 : (v.isInt() ? 1 : 0)),(float)v.min(),maxEx,v.length(),isBinom);
  }
  return hs;
}
