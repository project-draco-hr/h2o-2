{
  ddplyPass1 p1=ddplyPass1.PASS1TMP.remove(_p1key);
  if (p1._groups != null) {
    Futures fs=new Futures();
    for (int i=0; i < _dss.length; i++) {
      Group g=new Group(_dss[i]);
      NewChunk nc=p1._groups.get(g);
      if (nc != null) {
        nc._vec=_avs[i];
        nc.close(H2O.SELF.index(),fs);
      }
    }
    fs.blockForPending();
  }
  _p1key=null;
  _dss=null;
  tryComplete();
}
