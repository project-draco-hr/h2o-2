{
  if (arg == _x) {
    int[] ii=_x.value();
    if (ii == null)     throw new IllegalArgumentException("Cannot ignore all columns");
    int numSelect=ii == null ? 0 : ii.length;
    if (_key.value() != null && numSelect > _key.value()._numrows - 1)     throw new IllegalArgumentException("Cannot have more columns than degrees of freedom = " + String.valueOf(_key.value()._numrows - 1));
  }
}
