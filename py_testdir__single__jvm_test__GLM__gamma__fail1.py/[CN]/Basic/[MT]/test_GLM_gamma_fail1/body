def test_GLM_gamma_fail1(self):
    csvPathname = 'UCI/UCI-large/covtype/covtype.data'
    parseResult = h2i.import_parse(bucket='datasets', path=csvPathname, schema='put')
    for trial in range(5):
        kwargs = {'standardize': 1, 'family': 'gamma', 'link': 'familyDefault', 'y': 54, 'lambda': 0.0001, 'alpha': 0.5, 'max_iter': 25, 'n_folds': 1, }
        start = time.time()
        glm = h2o_cmd.runGLM(timeoutSecs=120, parseResult=parseResult, **kwargs)
        print 'glm end on ', csvPathname, 'took', (time.time() - start), 'seconds'
        h2o_glm.simpleCheckGLM(self, glm, None, None, **kwargs)
        print 'Trial #', trial, 'completed\n'
