{
  _arys++;
  int len=get4();
  if (len == -1)   return null;
  Class<T[]> tcA=(Class<T[]>)Array.newInstance(tc,0).getClass();
  Class<T[][]> tcAA=(Class<T[][]>)Array.newInstance(tcA,0).getClass();
  T[][][] ts=(T[][][])Array.newInstance(tcAA,len);
  for (int i=0; i < len; ++i)   ts[i]=getAA(tc);
  return ts;
}
