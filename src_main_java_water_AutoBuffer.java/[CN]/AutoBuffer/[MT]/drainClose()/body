{
  try {
    try {
      Log.info("drain-closing channel to " + ((SocketChannel)_chan).socket().getInetAddress());
    }
 catch (    Throwable t) {
      Log.info("drain-closing channel to unknown node");
    }
    _chan.close();
    restorePriority();
    bbFree();
  }
 catch (  IOException e) {
    throw Log.errRTExcept(e);
  }
}
