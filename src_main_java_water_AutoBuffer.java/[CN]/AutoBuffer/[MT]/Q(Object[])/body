{
  long ctm=System.currentTimeMillis();
  if (ctm < NOISE + 500)   return;
  if (A == null)   return;
  int x=0;
  for (; x < A.length; x++)   if (A[x] != null)   break;
  int y=A.length;
  for (; y > x; y--)   if (A[y - 1] != null)   break;
  int z=0;
  for (int i=x; i < y; i++)   if (A[i] == null)   z++;
  if (x + z + (A.length - y) <= (A.length >> 2))   return;
  NOISE=ctm;
}
