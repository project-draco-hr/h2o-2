def assertKeysExist(self, d, path, keys):
    path_elems = path.split('/')
    for path_elem in path_elems:
        if ('' != path_elem):
            assert (path_elem in d), ((('Failed to find key: ' + path) + ' in dict: ') + repr(d))
            d = d[path_elem]
    for key in keys:
        assert (key in d), ((('Failed to find key: ' + key) + ' in dict: ') + repr(d))
