{
  final int cidx=chk._cidx;
  while (cidx >= _espc.length) {
    _espc=Arrays.copyOf(_espc,_espc.length << 1);
    _chunkTypes=Arrays.copyOf(_chunkTypes,_chunkTypes.length << 1);
  }
  _espc[cidx]=chk._len;
  _chunkTypes[cidx]=chk.type();
  _missingCnt+=chk._naCnt;
  _strCnt+=chk._strCnt;
  _totalCnt+=chk._len;
}
