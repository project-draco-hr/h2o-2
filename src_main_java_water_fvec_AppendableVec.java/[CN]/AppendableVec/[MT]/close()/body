{
  int nchunk=_espc.length;
  while (nchunk > 0 && _espc[nchunk - 1] == 0)   nchunk--;
  long espc[]=new long[nchunk + 1];
  long x=0;
  for (int i=0; i < nchunk; i++) {
    espc[i]=x;
    x+=_espc[i];
  }
  espc[nchunk]=x;
  Vec vec=new Vec(_key,espc,_min,_max,_sum);
  DKV.put(_key,vec);
  return vec;
}
