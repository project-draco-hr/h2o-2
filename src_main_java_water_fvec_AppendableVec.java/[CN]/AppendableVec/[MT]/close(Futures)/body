{
  int nchunk=_espc.length;
  while (nchunk > 0 && _espc[nchunk - 1] == 0)   nchunk--;
  long espc[]=new long[nchunk + 1];
  long x=0;
  for (int i=0; i < nchunk; i++) {
    espc[i]=x;
    x+=_espc[i];
  }
  espc[nchunk]=x;
  System.out.println("Closing AV w/" + x + " rows="+ espc);
  Vec vec=new Vec(_key,espc,!_hasFloat,_min,_max);
  DKV.put(_key,vec,fs);
  return vec;
}
