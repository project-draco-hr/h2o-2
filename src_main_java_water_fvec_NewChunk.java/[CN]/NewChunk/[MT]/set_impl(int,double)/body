{
  if (_ls != null) {
    if (_len2 != _len)     throw H2O.unimpl();
    double ds[]=MemoryManager.malloc8d(_len);
    for (int j=0; j < _len; j++)     ds[j]=(isNA(j) || isEnum(j)) ? Double.NaN : _ls[j] * Math.pow(10,_xs[j]);
    _ds=ds;
    _ls=null;
    _xs=null;
  }
  _ds[i]=d;
  return true;
}
