{
  int validCols=_drf.source.numCols() - 1;
  if (validCols < _drf.drfParams.num_split_features) {
    Log.info(Log.Tag.Sys.RANDF,"Limiting features from " + _drf.drfParams.num_split_features + " to "+ validCols+ " because there are no more valid columns in the dataset");
    _drf.drfParams.num_split_features=validCols;
  }
}
