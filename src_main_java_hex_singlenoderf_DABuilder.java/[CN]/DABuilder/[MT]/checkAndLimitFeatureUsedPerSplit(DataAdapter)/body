{
  int validCols=_drf._rfmodel.fr.numCols() - 1;
  if (validCols < _drf._params._numSplitFeatures) {
    Log.warn(Log.Tag.Sys.RANDF,"Limiting features from " + _drf._params._numSplitFeatures + " to "+ validCols+ " because there are no more valid columns in the dataset");
    _drf._params._numSplitFeatures=validCols;
  }
}
