{
  Chunk c=null;
  for (  Chunk x : chks)   if (x.getClass() == water.fvec.C2Chunk.class) {
    c=x;
    break;
  }
  assertNotNull("Expect to find a C2Chunk",c);
  assertTrue(c.writable());
  double d=c._vec.min();
  for (int i=0; i < c._len; i++) {
    double e=c.at0(i);
    c.set80(i,d);
    d=e;
  }
}
