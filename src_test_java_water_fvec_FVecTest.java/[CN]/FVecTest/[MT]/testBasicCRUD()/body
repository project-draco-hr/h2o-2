{
  File file=TestUtil.find_test_file("./target/h2o.jar");
  Key key=NFSFileVec.make(file);
  NFSFileVec nfs=DKV.get(key).get();
  System.err.println(nfs.at(0));
  int[] x=new ByteHisto().invoke(nfs)._x;
  int sum=0;
  for (  int i : x)   sum+=i;
  assertEquals(file.length(),sum);
  UKV.remove(key);
}
