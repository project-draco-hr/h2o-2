{
  try {
    while (true) {
      Future f=null;
synchronized (this) {
        if (_pending_cnt == 0)         return;
        f=_pending[--_pending_cnt];
      }
      f.get();
    }
  }
 catch (  InterruptedException ie) {
    throw new RuntimeException(ie);
  }
catch (  ExecutionException ee) {
    throw new RuntimeException(ee);
  }
}
