{
  Value v=DKV.get(k);
  if (v != null && !v.isArray()) {
    if (v._max > 100)     throw new EvaluationException(pos,"Key " + k + " is neither a Vector nor a simple number");
    String s=new String(v.memOrLoad());
    try {
      _const=Double.valueOf(s);
    }
 catch (    NumberFormatException nfe) {
      throw L.err(new EvaluationException(pos,"Key " + k + " is neither a Vector nor a simple number"));
    }
    _key=null;
    _refCount=1;
    _type=Type.rtNumberLiteral;
  }
 else {
    _const=0;
    _key=k;
    _refCount=refCount;
    _type=Type.rtKey;
  }
  _colIndex=-1;
  _str=null;
}
