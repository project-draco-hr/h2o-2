{
  if (ls.length > 65536) {
    System.out.println("len=" + ls.length + " nz="+ nzcnt);
    throw H2O.unimpl();
  }
  byte[] buf=new byte[nzcnt * (2 + 2) + OFF];
  UDP.set2(buf,0,(short)ls.length);
  int j=OFF;
  for (int i=0; i < ls.length; i++)   if (ls[i] != 0 || xs[i] != 0)   j+=UDP.set2(buf,j,(short)i) + UDP.set2(buf,j + 2,(short)(ls[i] == 0 ? C2Chunk._NA : ls[i] * DParseTask.pow10(xs[i])));
  return buf;
}
