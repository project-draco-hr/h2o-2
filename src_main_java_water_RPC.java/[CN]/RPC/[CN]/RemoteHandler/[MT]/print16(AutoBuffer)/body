{
  int flag=ab.getFlag();
  String clazz="";
  if (flag == CLIENT_UDP_SEND)   clazz=new String(ab.getA1(Math.min(ab.get2(),ab.remaining())));
  String fs="";
switch (flag) {
case SERVER_UDP_SEND:
    fs="SERVER_UDP_SEND";
  break;
case SERVER_TCP_SEND:
fs="SERVER_TCP_SEND";
break;
case CLIENT_UDP_SEND:
fs="CLIENT_UDP_SEND";
break;
case CLIENT_TCP_SEND:
fs="CLIENT_TCP_SEND";
break;
}
return "task# " + ab.getTask() + " "+ fs+ " "+ clazz;
}
