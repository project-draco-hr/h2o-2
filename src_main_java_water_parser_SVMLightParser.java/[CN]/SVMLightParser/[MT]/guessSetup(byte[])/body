{
  InputStream is=new ByteArrayInputStream(bits);
  SVMLightParser p=new SVMLightParser(new ParserSetup(ParserType.SVMLight,CsvParser.AUTO_SEP,false));
  InspectDataOut dout=new InspectDataOut();
  try {
    p.streamParse(is,dout);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return new PSetupGuess(new ParserSetup(ParserType.SVMLight,CsvParser.AUTO_SEP,dout._ncols,false,null,false),dout._nlines,dout._invalidLines,dout.data(),dout.errors());
}
