{
  Field targetField;
  try {
    targetField=target.getClass().getField(f.getName());
  }
 catch (  Throwable e) {
    return false;
  }
  if (targetField == null)   return false;
  try {
    targetField.set(target,f.get(source));
  }
 catch (  Throwable e) {
    return false;
  }
  return true;
}
