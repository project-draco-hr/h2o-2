{
  Key key=(Key)modelKey;
  String sk=key.toString();
  Value v=DKV.get(key);
  if (v == null)   throw new IllegalArgumentException("Key " + sk + " not found!");
  Model M=null;
  try {
    M=v.get();
  }
 catch (  Throwable t) {
    throw new IllegalArgumentException("Key " + sk + " is not a Model key");
  }
  return scoreModel(M,colNames,row);
}
