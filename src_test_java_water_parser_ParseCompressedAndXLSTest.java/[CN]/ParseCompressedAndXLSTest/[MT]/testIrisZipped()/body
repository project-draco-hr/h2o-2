{
  Key k1=null, k2=null;
  try {
    k2=loadAndParseKey("csv.hex","smalldata/iris/iris.csv");
    k1=loadAndParseKey("xls.hex","smalldata/iris/iris.csv.zip");
    Value v1=DKV.get(k1);
    Value v2=DKV.get(k2);
    assertTrue(v1.isBitIdentical(v2));
  }
  finally {
    if (k1 != null)     UKV.remove(k1);
    if (k2 != null)     UKV.remove(k2);
  }
}
