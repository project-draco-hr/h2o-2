{
  H2O.main(args);
  int nodes=Integer.parseInt(args[6]);
  String job=args[7];
  TestUtil.stall_till_cloudsize(nodes);
  System.out.println("Cloud is up");
  System.out.println("Go to http://127.0.0.1:54321");
  if (!job.equals("null")) {
    String pack=job.substring(0,job.lastIndexOf('.'));
    LaunchJar.weavePackages(pack);
    Class<Job> c=(Class)Class.forName(job);
    c.newInstance().fork();
  }
}
