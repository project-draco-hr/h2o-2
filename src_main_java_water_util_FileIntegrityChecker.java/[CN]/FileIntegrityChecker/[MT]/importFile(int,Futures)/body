{
  if (_ok[i] < H2O.CLOUD.size())   return null;
  File f=new File(_files[i]);
  Key k=PersistNFS.decodeFile(f);
  long size=f.length();
  Value val=(size < 2 * ValueArray.CHUNK_SZ) ? new Value(k,(int)size,Value.NFS) : new Value(k,new ValueArray(k,size),Value.NFS);
  val.setdsk();
  UKV.put(k,val,fs);
}
