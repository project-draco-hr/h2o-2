{
  if (!isPersisted())   return;
  clrdsk();
switch (_persist & BACKEND_MASK) {
case ICE:
    PersistIce.fileDelete(this);
  break;
case HDFS:
PersistHdfs.fileDelete(this);
break;
case NFS:
PersistNFS.fileDelete(this);
break;
case S3:
PersistS3.fileDelete(this);
break;
default :
throw H2O.unimpl();
}
}
