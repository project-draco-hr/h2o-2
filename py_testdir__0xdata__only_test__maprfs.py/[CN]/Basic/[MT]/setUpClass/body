@classmethod
def setUpClass(cls):
    global localhost
    localhost = h2o.decide_if_localhost()
    if localhost:
        h2o.build_cloud(1, use_maprfs=True, hdfs_version='mapr2.1.3', hdfs_name_node='mr-0x1.0xdata.loc:7222')
    else:
        h2o_hosts.build_cloud_with_hosts(1, use_maprfs=True, hdfs_version='mapr2.1.3', hdfs_name_node='mr-0x1.0xdata.loc:7222')
