{
  int[] ties=new int[preds.length];
  int best=0;
  int tieCnt=0;
  ties[tieCnt]=0;
  for (int c=1; c < preds.length; c++) {
    if (preds[best] < preds[c]) {
      best=c;
      ties[tieCnt=0]=c;
    }
 else     if (preds[best] == preds[c]) {
      ties[++tieCnt]=c;
    }
  }
  if (tieCnt >= 1) {
    best=ties[row % (tieCnt + 1)];
  }
  return best;
}
