{
  int numCorrect=0;
  int numWrong=0;
  double[] xValues=new double[numInput];
  double[] tValues=new double[numOutput];
  double[] yValues;
  for (int i=0; i < testData.length; ++i) {
    System.arraycopy(testData[i],0,xValues,0,numInput);
    System.arraycopy(testData[i],numInput,tValues,0,numOutput);
    yValues=this.ComputeOutputs(xValues);
    float[] yValues_float=new float[yValues.length];
    for (int j=0; j < yValues.length; ++j)     yValues_float[j]=(float)yValues[j];
    int maxIndex=MaxIndexWithTieBreaking(yValues_float,i);
    if (tValues[maxIndex] == 1.0)     ++numCorrect;
 else     ++numWrong;
  }
  return (double)numWrong / (numCorrect + numWrong);
}
