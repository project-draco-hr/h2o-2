{
  PSetup setup=_source.value();
  if (setup == null)   return null;
  String n=setup._key.toString();
  int dot=n.lastIndexOf('.');
  if (dot > 0)   n=n.substring(0,dot);
  int i=0;
  String res=n + ".hex";
  Key k=Key.make(res);
  while (DKV.get(k) != null)   k=Key.make(res=n + ++i + ".hex");
  return res;
}
