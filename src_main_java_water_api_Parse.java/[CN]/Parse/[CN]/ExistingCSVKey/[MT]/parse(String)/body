{
  Key k=Key.make(input);
  Value v=DKV.get(k);
  if (v == null)   throw new IllegalArgumentException("Key " + input + " not found!");
  CsvParser.Setup setup=Inspect.csvGuessValue(v);
  if (setup._data == null || setup._data[0].length == 0)   throw new IllegalArgumentException("The dataset format is not recognized/supported");
  return new PSetup(k,setup);
}
