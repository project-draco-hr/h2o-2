{
  int validCols=_drf._rfmodel._va._cols.length - 1;
  if (validCols < _drf._numSplitFeatures) {
    L.warn(this,Sys.RANDF,"Limiting features from " + _drf._numSplitFeatures + " to "+ validCols+ " because there are no more valid columns in the dataset");
    _drf._numSplitFeatures=validCols;
  }
}
