{
  StringBuilder sb=new StringBuilder();
  sb.append(_name).append("\n");
  for (int i=0; i < _bins.length; i++)   sb.append(String.format("cnt=%d, min=%f, max=%f, mean=%f, var=%f\n",_bins[i],_mins[i],_maxs[i],_Ms == null ? Double.NaN : mean(i),_Ms == null ? Double.NaN : var(i)));
  return sb.toString();
}
