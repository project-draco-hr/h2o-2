{
  double[][] res=new double[k][];
  res[0]=points[0];
  int count=1;
  ClusterDist cd=new ClusterDist();
  while (count < res.length) {
    double max=0;
    int index=0;
    for (int i=0; i < points.length; i++) {
      double sqr=minSqr(res,points[i],cd,count);
      if (sqr > max) {
        max=sqr;
        index=i;
      }
    }
    res[count++]=points[index];
  }
  return res;
}
