def test_G_Slower_JUNIT(self):
    h2o.tear_down_cloud()
    h2o.build_cloud(node_count=3)
    (ps, stdout, stderr) = h2o.spawn_cmd('junit', ['java', ('-Dh2o.arg.ice_root=' + h2o.tmp_dir('ice.')), ('-Dh2o.arg.name=' + h2o.cloud_name()), ('-Dh2o.arg.ip=' + h2o.get_ip_address()), '-ea', '-jar', h2o.find_file('target/h2o.jar'), '-mainClass', 'org.junit.runner.JUnitCore', 'water.ConcurrentKeyTest', 'hex.MinorityClassTest', 'water.exec.RBigDataTest'])
    rc = ps.wait(None)
    out = file(stdout).read()
    err = file(stderr).read()
    if (rc is None):
        ps.terminate()
        raise Exception(('junit timed out.\nstdout:\n%s\n\nstderr:\n%s' % (out, err)))
    elif (rc != 0):
        raise Exception(('junit failed.\nstdout:\n%s\n\nstderr:\n%s' % (out, err)))
