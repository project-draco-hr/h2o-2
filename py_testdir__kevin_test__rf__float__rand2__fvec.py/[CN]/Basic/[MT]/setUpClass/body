@classmethod
def setUpClass(cls):
    global SEED, localhost
    SEED = h2o.setup_random_seed(seed=4548383660131500992)
    localhost = h2o.decide_if_localhost()
    if localhost:
        h2o.build_cloud(2, java_heap_MB=1300, use_flatfile=True)
    else:
        import h2o_hosts
        h2o_hosts.build_cloud_with_hosts()
