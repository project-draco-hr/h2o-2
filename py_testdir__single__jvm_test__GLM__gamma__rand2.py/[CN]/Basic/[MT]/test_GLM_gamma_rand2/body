def test_GLM_gamma_rand2(self):
    csvPathname = h2o.find_dataset('UCI/UCI-large/covtype/covtype.data')
    parseResult = h2o_cmd.parseFile(csvPathname=csvPathname)
    paramDict = define_params()
    for trial in range(10):
        params = {'y': 54, 'n_folds': 3, 'family': 'gamma', 'alpha': 0.5, 'lambda': 0.0001, 'max_iter': 24, }
        colX = h2o_glm.pickRandGlmParams(paramDict, params)
        kwargs = params.copy()
        start = time.time()
        glm = h2o_cmd.runGLMOnly(timeoutSecs=300, parseResult=parseKey, **kwargs)
        print 'glm end on ', csvPathname, 'took', (time.time() - start), 'seconds'
        h2o_glm.simpleCheckGLM(self, glm, None, **kwargs)
        print 'Trial #', trial, 'completed\n'
