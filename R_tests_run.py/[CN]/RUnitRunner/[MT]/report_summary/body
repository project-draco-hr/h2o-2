def report_summary(self):
    '\n        Report some summary information when the tests have finished running.\n\n        @return: none\n        '
    passed = 0
    failed = 0
    notrun = 0
    total = 0
    for test in self.tests:
        if test.get_passed():
            passed += 1
        elif test.get_completed():
            failed += 1
        else:
            notrun += 1
        total += 1
    self.log('')
    self.log('----------------------------------------------------------------------')
    self.log('')
    self.log('SUMMARY OF RESULTS')
    self.log('')
    self.log('----------------------------------------------------------------------')
    self.log('')
    self.log(('Total tests:      ' + str(total)))
    self.log(('Passed:           ' + str(passed)))
    self.log(('Did not pass:     ' + str(failed)))
    self.log(('Did not complete: ' + str(notrun)))
