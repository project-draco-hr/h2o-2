def start_clouds(self):
    '\n        Start all H2O clouds.\n\n        @return: none\n        '
    if self.terminated:
        return
    for cloud in self.clouds:
        if self.terminated:
            return
        cloud.start()
    print 'Waiting for H2O nodes to come up...'
    for cloud in self.clouds:
        if self.terminated:
            return
        cloud.wait_for_cloud_to_be_up()
