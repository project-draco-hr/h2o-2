def _report_test_result(self, test):
    port = test.get_port()
    if test.get_passed():
        s = ('PASS      %d %-70s' % (port, test.get_test_name()))
        self._log(s)
    else:
        s = ('     FAIL %d %-70s %s' % (port, test.get_test_name(), test.get_output_dir_file_name()))
        self._log(s)
