{
  try {
    ByteArrayOutputStream bos=new ByteArrayOutputStream();
    ObjectOutput out=null;
    try {
      out=new ObjectOutputStream(bos);
      out.writeObject(this);
      return Base64.encodeBase64String(bos.toByteArray());
    }
  finally {
      out.close();
      bos.close();
    }
  }
 catch (  Exception ex) {
    throw Log.errRTExcept(ex);
  }
}
