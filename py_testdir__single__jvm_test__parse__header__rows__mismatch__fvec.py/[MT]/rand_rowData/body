def rand_rowData(totalCols):
    rowData1 = str(random.randint(0, 7))
    for i in range(totalCols):
        rowData1 = ((rowData1 + ',') + str(random.randint(0, 7)))
    rowData2 = str(random.randint(0, 7))
    for i in range(totalCols):
        rowData2 = ((rowData2 + ',') + str(random.randint(0, 7)))
    r = [rowData1, rowData2]
    return r
