{
  Chunk ys=DTree.chk_resp(chks,_ncols,_nclass);
  double ds[]=new double[_nclass];
  for (int row=0; row < ys._len; row++) {
    double sum=0;
    for (int k=0; k < _nclass; k++)     sum+=(ds[k]=Math.exp(tree(chks,k).at0(row)));
    for (int k=0; k < _nclass; k++)     work(chks,k).set0(row,ds[k] / sum);
  }
}
