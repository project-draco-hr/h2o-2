{
  int nid=0;
  double pred=Double.NaN;
  while (true) {
    Tree.Node node=_tree.n(nid);
    Histogram h=node._hs[node._col];
    if (h == null)     break;
    double d=chks[node._col].at0(i);
    int bin=h.bin(d);
    pred=h.mean(bin);
    if (node._ns == null)     break;
    nid=node._ns[bin];
    if (nid == -1)     break;
  }
  return pred;
}
