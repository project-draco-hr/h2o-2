{
  GBMModel model=new GBMModel(outputKey,dataKey,testKey,names,domains,ntrees);
  DKV.put(outputKey,model);
  int tid;
  DTree[] ktrees=null;
  TreeStats tstats=new TreeStats();
  for (tid=0; tid < ntrees; tid++) {
    new ComputeProb().doAll(fr);
    new ComputeRes().doAll(fr);
    ktrees=buildNextKTrees(fr);
    if (cancelled())     break;
    tstats.updateBy(ktrees);
    model=doScoring(model,outputKey,fr,ktrees,tid,tstats);
  }
  model=doScoring(model,outputKey,fr,ktrees,tid,tstats);
  cleanUp(fr,t_build);
}
