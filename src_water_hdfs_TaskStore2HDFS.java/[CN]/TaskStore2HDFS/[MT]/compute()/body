{
  String path=null;
  ValueArray ary=ValueArray.value(_arykey);
  Key self=selfKey();
  while (_indexFrom < ary.chunks()) {
    Key ckey=ary.getChunkKey(_indexFrom++);
    if (!ckey.home()) {
      RPC.call(chunkHome(),this);
      return;
    }
    Value val=DKV.get(ckey);
    _err=PersistHdfs.appendChunk(_arykey,val);
    if (_err != null)     return;
    UKV.put(self,this);
  }
  UKV.remove(self);
}
