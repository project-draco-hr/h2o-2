{
  RPC<TaskGetKey> rpc;
  while (true) {
    rpc=TGKS.get(key);
    if (rpc != null)     break;
    rpc=new RPC(target,new TaskGetKey(key,priority));
    if (TGKS.putIfMatchUnlocked(key,rpc,null) == null) {
      rpc.call();
      break;
    }
  }
  Value val=rpc.get()._val;
  TGKS.putIfMatchUnlocked(key,null,rpc);
  return val;
}
