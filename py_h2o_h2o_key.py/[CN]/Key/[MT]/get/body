def get(self):
    ' Returns the value of the key. If the value is cached, returns the cached value, otherwise connects to the\n        server to get the value. You may always call the invalidate() method to \n        '
    if (self._value == None):
        try:
            res = self._h2o._remoteSend(PAGE_GET, {KEY: self._name, })
            self._value = self._h2o._keyToDict(res)
        except H2OException:
            pass
    return self._value
