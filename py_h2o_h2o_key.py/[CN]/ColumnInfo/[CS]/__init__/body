def __init__(self, res):
    ' Creates the ColumnInfo object from the resource dictionary (i.e. one returned by the JSON api) '
    self._scale = res['scale']
    self._offset = res['off']
    self.name = res['name']
    self.min = res['min']
    self.max = res['max']
    self.badAt = res['badat']
    self._base = res['base']
    self.var = res['var']
    self.mean = res['mean']
    self.type = res['type']
    self._size = res['size']
