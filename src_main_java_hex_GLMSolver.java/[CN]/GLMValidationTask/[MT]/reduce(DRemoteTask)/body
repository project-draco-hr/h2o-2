{
  GLMValidationTask other=(GLMValidationTask)drt;
  _n+=other._n;
  _nullDeviance+=other._nullDeviance;
  _deviance+=other._deviance;
  _err+=other._err;
  _caseCount+=other._caseCount;
  if (_cm != null) {
    for (int i=0; i < _thresholds.length; ++i)     _cm[i].add(other._cm[i]);
  }
 else   _cm=other._cm;
}
