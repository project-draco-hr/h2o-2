{
  long sum=0;
  int cnt=0;
  for (  DSharedHistogram hss[][] : _hcs)   if (hss != null)   for (  DSharedHistogram hs[] : hss)   if (hs != null)   for (  DSharedHistogram h : hs)   if (h != null) {
    sum+=h.byteSize();
    cnt++;
  }
  System.out.println("Going to ship " + PrettyPrint.bytes(sum) + " in "+ cnt+ " Histograms");
}
