{
  if (validation == null)   return doAll(fr);
  Frame res=model.score(validation,true);
  Frame frs[]=model.adapt(validation,true);
  Frame fr2=frs[0];
  fr2.add("response",vresponse);
  for (int i=0; i < _nclass; i++)   fr2.add("Work" + i,res.vecs()[i + 1]);
  doAll(fr2);
  frs[1].remove();
  return this;
}
