def file_append(infile, outfile):
    h2o.verboseprint("\nAppend'ing", infile, 'to', outfile)
    start = time.time()
    in_file = open(infile, 'rb')
    out_file = open(outfile, 'a')
    out_file.write(in_file.read())
    out_file.close()
    h2o.verboseprint('\nAppend took', (time.time() - start), 'secs')
