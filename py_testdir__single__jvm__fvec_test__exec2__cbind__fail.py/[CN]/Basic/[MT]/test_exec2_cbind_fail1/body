def test_exec2_cbind_fail1(self):
    for i in range(5):
        execExpr = 'a=c(0,0,0)'
        h2e.exec_expr(execExpr=execExpr, timeoutSecs=30)
        execExpr = 'b = a'
        h2e.exec_expr(execExpr=execExpr, timeoutSecs=30)
        execExpr = 'h <- cbind(a, b)'
        h2e.exec_expr(execExpr=execExpr, timeoutSecs=30)
    h2o.check_sandbox_for_errors()
