{
  DocGen.HTML.section(sb,"Tree stats");
  DocGen.HTML.arrayHead(sb);
  sb.append("<tr><th>&nbsp;</th>").append("<th>Min</th><th>Mean</th><th>Max</th></tr>");
  sb.append("<tr><th>Depth</th>").append("<td>").append(treeStats.minDepth).append("</td>").append("<td>").append(treeStats.meanDepth).append("</td>").append("<td>").append(treeStats.maxDepth).append("</td></tr>");
  sb.append("<th>Leaves</th>").append("<td>").append(treeStats.minLeaves).append("</td>").append("<td>").append(treeStats.meanLeaves).append("</td>").append("<td>").append(treeStats.maxLeaves).append("</td></tr>");
  DocGen.HTML.arrayTail(sb);
}
