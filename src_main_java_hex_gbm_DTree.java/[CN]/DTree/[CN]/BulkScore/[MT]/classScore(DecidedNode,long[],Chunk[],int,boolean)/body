{
  int bin=prev.bin(chks,i);
  float[] ycls=prev._ycls[bin];
  boolean empty=true;
  for (int c=0; c < _nclass; c++) {
    clss[c]+=ycls[c];
    if (empty && ycls[c] != 0.0f)     empty=false;
  }
  if (empty && prev._pid != -1) {
    assert recur;
    prev=prev._tree.decided(prev._pid);
    classScore(prev,clss,chks,i,false);
  }
}
