{
  if (_split._col == -1)   return "Decided has col = -1";
  int col=_split._col;
  double preds[]=_split._preds;
  if (_split._equal)   return _tree._names[col] + " != " + _splat+ " = "+ preds[0]+ "\n"+ _tree._names[col]+ " == "+ _splat+ " = "+ preds[1]+ "\n";
  return _tree._names[col] + " < " + _splat+ " = "+ preds[0]+ "\n"+ _splat+ " <="+ _tree._names[col]+ " = "+ preds[1]+ "\n";
}
