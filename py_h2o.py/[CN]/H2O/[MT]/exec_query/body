def exec_query(self, timeoutSecs=20, **kwargs):
    params_dict = {'expression': None, }
    browseAlso = kwargs.pop('browseAlso', False)
    params_dict.update(kwargs)
    verboseprint('\nexec_query:', params_dict)
    a = self.__check_request(requests.get(url=self.__url('Exec.json'), timeout=timeoutSecs, params=params_dict))
    verboseprint('\nexec_query result:', dump_json(a))
    return a
