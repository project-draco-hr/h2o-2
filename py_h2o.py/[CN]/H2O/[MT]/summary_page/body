def summary_page(self, key, timeoutSecs=60, noPrint=True, **kwargs):
    if beta_features:
        params_dict = {'source': key, 'cols': None, 'max_ncols': 1000, }
    else:
        params_dict = {'key': key, 'x': None, 'max_column_display': 1000, }
    browseAlso = kwargs.pop('browseAlso', False)
    check_params_update_kwargs(params_dict, kwargs, 'summary_page', print_params=True)
    a = self.__do_json_request(('2/SummaryPage2.json' if beta_features else 'SummaryPage.json'), timeout=timeoutSecs, params=params_dict)
    verboseprint('\nsummary_page result:', dump_json(a))
    if (not beta_features):
        h2o_cmd.infoFromSummary(a, noPrint=noPrint)
    return a
