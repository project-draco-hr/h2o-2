def h2o_log_msg(self, message=None):
    if (1 == 0):
        return
    if (not message):
        message = '\n'
        message += '\n#***********************'
        message += ('\nTest Start: ' + python_test_name)
        message += '\n#***********************'
    params = {'message': message, }
    self.__do_json_request('2/LogAndEcho', params=params)
