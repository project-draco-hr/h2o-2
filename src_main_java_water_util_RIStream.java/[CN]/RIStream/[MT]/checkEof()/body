{
  if (_knownSize)   if (_off < _expectedSz)   Log.warn("premature end of file reported, expected " + _expectedSz + " bytes, but got eof after "+ _off+ " bytes");
 else   Log.warn("[Expected] eof after " + _expectedSz + " bytes");
 else   Log.warn("eof after " + _off + " bytes, expected size unknown");
}
