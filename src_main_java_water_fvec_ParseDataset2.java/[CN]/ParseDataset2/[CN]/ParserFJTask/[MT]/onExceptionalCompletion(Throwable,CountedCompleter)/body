{
  Futures fs=new Futures();
  for (  Key k : _keys)   UKV.remove(k,fs);
  if (_job != null) {
    UKV.remove(_job.destination_key,fs);
    UKV.remove(_job._progress,fs);
    if (_job._mfpt != null)     _job._mfpt.onExceptionCleanup(fs);
  }
  fs.blockForPending();
  if (_job != null)   _job.cancel(ex);
  return true;
}
