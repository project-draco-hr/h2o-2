{
  GLMParams res=new GLMParams(_family.value(),_link.value());
  if (res._link == Link.familyDefault)   res._link=res._family.defaultLink;
  res._maxIter=_maxIter.value();
  res._betaEps=_betaEps.value();
  if (_caseWeight.valid())   res._caseWeight=_caseWeight.value();
  if (_case.valid())   res._caseVal=_case.value();
  res._caseMode=_caseMode.valid() ? _caseMode.value() : CaseMode.none;
  return res;
}
