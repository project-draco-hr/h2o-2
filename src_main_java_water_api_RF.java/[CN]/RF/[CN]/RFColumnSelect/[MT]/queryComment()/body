{
  TreeSet<String> ignoredCols=_constantColumns.get();
  StringBuilder sb=new StringBuilder();
  if (_badColumns.get() != null && _badColumns.get() > 0)   sb.append("<b> There are ").append(_badColumns.get()).append(" columns with more than ").append(_maxNAsRatio * 100).append("% of NAs.<br/>");
  if (ignoredCols != null && !ignoredCols.isEmpty())   sb.append("Ignoring ").append(_constantColumns.get().size()).append(" constant columns</b>: ").append(ignoredCols.toString());
  if (sb.length() > 0)   sb.insert(0,"<div class='alert'>").append("</div>");
  return sb.toString();
}
