{
  final int rows=y.size();
  assert(res.size() == rows);
  for (int r=0; r < rows; r++) {
    res.set(r,0);
    if (row_bits != null && (row_bits[r / 8] & (1 << (r % 8))) == 0)     continue;
    for (SparseVector.Iterator it=x.begin(); !it.equals(x.end()); it.next())     res.add(r,a.get(r,it.index()) * it.value());
    res.add(r,y.get(r));
  }
}
