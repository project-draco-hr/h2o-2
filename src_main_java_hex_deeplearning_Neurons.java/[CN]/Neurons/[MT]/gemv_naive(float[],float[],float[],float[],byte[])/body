{
  final int cols=x.length;
  final int rows=y.length;
  assert(res.length == rows);
  for (int r=0; r < rows; r++) {
    res[r]=0;
    if (row_bits != null && (row_bits[r / 8] & (1 << (r % 8))) == 0)     continue;
    for (int i=0; i < cols; i++)     res[r]+=a[r * cols + i] * x[i];
    res[r]+=y[r];
  }
}
