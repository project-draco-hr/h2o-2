def plotit(xList, eList, sList):
    if (h2o.python_username != 'kevin'):
        return
    import pylab as plt
    if eList:
        print 'xList', xList
        print 'eList', eList
        print 'sList', sList
        label = '1jvmx28GB Covtype GBM learn_rate=.2 nbins=1024 ntrees=40 min_rows = 10'
        plt.figure()
        plt.plot(xList, eList)
        plt.xlabel('max_depth')
        plt.ylabel('error')
        plt.title(label)
        plt.draw()
        label = '1jvmx28GB Covtype GBM learn_rate=.2 nbins=1024 ntrees=40 min_rows = 10'
        plt.figure()
        plt.plot(xList, sList)
        plt.xlabel('max_depth')
        plt.ylabel('time')
        plt.title(label)
        plt.draw()
        plt.show()
