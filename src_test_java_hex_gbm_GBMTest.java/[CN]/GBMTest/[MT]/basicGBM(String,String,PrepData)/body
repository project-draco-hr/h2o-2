{
  File file=TestUtil.find_test_file(fname);
  if (file == null)   return;
  Key fkey=NFSFileVec.make(file);
  Key dest=Key.make(hexname);
  GBM gbm=null;
  Frame fr=null;
  try {
    gbm=new GBM();
    gbm.source=ParseDataset2.parse(dest,new Key[]{fkey});
    UKV.remove(fkey);
    gbm.response=prep.prep(gbm.source);
    gbm.source.add("response",gbm.response);
    gbm.ntrees=5;
    gbm.max_depth=5;
    gbm.learn_rate=0.1f;
    gbm.min_rows=10;
    gbm.nbins=100;
    gbm.cols=new int[gbm.source.numCols()];
    for (int i=0; i < gbm.cols.length; i++)     gbm.cols[i]=i;
    gbm.run();
    fr=gbm.score(gbm.source);
  }
  finally {
    UKV.remove(dest);
    if (gbm != null) {
      UKV.remove(gbm.dest());
      UKV.remove(gbm.response._key);
      gbm.remove();
      if (fr != null)       fr.remove();
    }
  }
}
