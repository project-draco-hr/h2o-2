{
  basicGBM("./smalldata/test/test_tree.csv","tree.hex",new PrepData(){
    Vec prep(    Frame fr){
      return fr.remove(1);
    }
  }
);
  basicGBM("./smalldata/test/test_tree_minmax.csv","tree_minmax.hex",new PrepData(){
    Vec prep(    Frame fr){
      return fr.remove("response");
    }
  }
);
  basicGBM("./smalldata/logreg/prostate.csv","prostate.hex",new PrepData(){
    Vec prep(    Frame fr){
      assertEquals(380,fr.numRows());
      UKV.remove(fr.remove("ID")._key);
      return fr.remove("CAPSULE");
    }
  }
);
  basicGBM("./smalldata/cars.csv","cars.hex",new PrepData(){
    Vec prep(    Frame fr){
      UKV.remove(fr.remove("name")._key);
      return fr.remove("cylinders");
    }
  }
);
  basicGBM("./smalldata/airlines/allyears2k_headers.zip","air.hex",new PrepData(){
    Vec prep(    Frame fr){
      return fr.remove("IsDepDelayed");
    }
  }
);
  basicGBM("../datasets/UCI/UCI-large/covtype/covtype.data","covtype.hex",new PrepData(){
    Vec prep(    Frame fr){
      assertEquals(581012,fr.numRows());
      for (      int ign : IGNS)       UKV.remove(fr.remove("C" + Integer.toString(ign))._key);
      return fr.remove(fr.numCols() - 1);
    }
  }
);
}
