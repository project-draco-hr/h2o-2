{
  try {
    if (model == null)     throw new IllegalArgumentException("Model is missing");
    Frame fr;
    if (model instanceof Model)     fr=((Model)model).score(data);
 else     fr=((OldModel)model).score(data);
    UKV.put(prediction,fr);
    return Inspect2.redirect(this,prediction.toString());
  }
 catch (  Throwable t) {
    Log.err(t);
    return Response.error(t.getMessage());
  }
}
