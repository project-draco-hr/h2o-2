{
  final int maxLen=10;
  StringBuilder builder=new StringBuilder();
  builder.append("TreeVotes [_trees=").append(_ntrees).append("\n, _treeCVotes=").append(_treeCVotes != null ? Arrays.toString(Arrays.copyOf(_treeCVotes,Math.min(_ntrees,maxLen))) : null).append("\n, _nrows=").append(_nrows != null ? Arrays.toString(Arrays.copyOf(_nrows,Math.min(_ntrees,maxLen))) : null).append("]");
  return builder.toString();
}
