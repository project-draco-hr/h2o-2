{
  String res=null;
  FSDataOutputStream s=null;
  try {
    s=_fs.append(getPathForKey(key));
    System.err.println("[hdfs] append=" + val.get().length);
    s.write(val.get());
  }
 catch (  IOException e) {
    res=e.getMessage();
  }
 finally {
    if (s != null)     try {
      s.close();
    }
 catch (    IOException e) {
    }
  }
  return res;
}
