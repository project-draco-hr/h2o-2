@classmethod
def setUpClass(cls):
    global localhost
    localhost = h2o.decide_if_localhost()
    h2o.beta_features = True
    if localhost:
        h2o.build_cloud(1, use_hdfs=True, hdfs_name_node='10.71.0.100', hdfs_version='cdh5', java_heap_GB=4, base_port=54321)
    else:
        h2o_hosts.build_cloud_with_hosts(1, base_port=54321)
