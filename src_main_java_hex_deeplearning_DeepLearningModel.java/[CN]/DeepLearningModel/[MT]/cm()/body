{
  final Errors lasterror=last_scored();
  if (errors == null)   return null;
  water.api.ConfusionMatrix cm=lasterror.validation ? lasterror.valid_confusion_matrix : lasterror.train_confusion_matrix;
  if (cm == null || cm.cm == null) {
    if (lasterror.validation) {
      return new ConfMat(lasterror.valid_err,lasterror.validAUC != null ? lasterror.validAUC.F1() : 0);
    }
 else {
      return new ConfMat(lasterror.train_err,lasterror.trainAUC != null ? lasterror.trainAUC.F1() : 0);
    }
  }
  long[][] newcm=new long[cm.cm.length - 1][cm.cm.length - 1];
  for (int i=0; i < newcm.length; ++i)   for (int j=0; j < newcm.length; ++j)   newcm[i][j]=cm.cm[i][j];
  return new hex.ConfusionMatrix(newcm);
}
