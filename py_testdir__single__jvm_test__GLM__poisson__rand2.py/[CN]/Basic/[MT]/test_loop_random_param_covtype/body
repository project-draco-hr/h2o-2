def test_loop_random_param_covtype(self):
    csvPathname = 'UCI/UCI-large/covtype/covtype.data'
    parseResult = h2i.import_parse(bucket='datasets', path=csvPathname, schema='put')
    paramDict = define_params()
    for trial in range(20):
        params = {'y': 54, 'n_folds': 3, 'family': 'poisson', 'alpha': 0.5, 'lambda': 0.0001, 'beta_epsilon': 0.001, 'max_iter': 15, }
        colX = h2o_glm.pickRandGlmParams(paramDict, params)
        kwargs = params.copy()
        timeoutSecs = (60 + (kwargs['n_folds'] * 20))
        timeoutSecs = max(timeoutSecs, (8 * (kwargs['max_iter'] + 1)))
        start = time.time()
        glm = h2o_cmd.runGLMOnly(timeoutSecs=timeoutSecs, parseResult=parseResult, **kwargs)
        print 'glm end on ', csvPathname, 'took', (time.time() - start), 'seconds'
        h2o_glm.simpleCheckGLM(self, glm, None, **kwargs)
        print 'Trial #', trial, 'completed\n'
